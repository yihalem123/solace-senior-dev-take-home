# Solace Senior Dev Take Home - Environment Variables Example
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Lambda Function Environment Variables
# These are automatically set by Terraform, but you can override them here
S3_BUCKET=your-s3-bucket-name
KMS_KEY_ID=your-kms-key-id

# Terraform Configuration
TF_VAR_aws_region=us-east-1
TF_VAR_project_name=solace-senior-dev-take-home
TF_VAR_environment=dev

# Optional: Use existing S3 bucket instead of creating new one
# TF_VAR_s3_bucket_name=your-existing-bucket-name

# Lambda Configuration (optional overrides)
TF_VAR_lambda_timeout=15
TF_VAR_lambda_memory_size=256

# Logging Configuration
TF_VAR_log_retention_days=14

# KMS Configuration
TF_VAR_kms_key_deletion_window=7 